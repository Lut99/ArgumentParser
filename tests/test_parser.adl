<date> {
    name "Date";
    pattern "<uint>" "-" "<uint>" "-" "<uint>";
}

<hello_world> {
    name "Hello, world!";
    pattern r"[A-Za-z_]+";
    source "hello_world.cpp";
}

---55 "regular expression!"

<regex_test> {
    name "Regex Test";
    pattern "<uint>" r"-\.\/" "<uint>" r"-\.\/" "<uint>";
}
